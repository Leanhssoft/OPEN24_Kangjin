    <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><i class="material-icons">close</i></button>
            <h4 class="modal-title" id="myModalLabel">
                Sửa nhóm phòng/bàn
            </h4>
        </div>
        <div class="modal-body floatleft add-phongban chinhanh-modal">
           
            <div class="form-group floatleft" data-bind="with: newNhomKhuVuc">
                <label class="form-label control-label req ng-binding">Tên nhóm <span class="red">(*)</span></label>
                <div class="form-wrap">
                    <input type="text" id="txtTenKhuVucedit" class="form-control ng-pristine ng-empty ng-invalid ng-invalid-required ng-touched" data-bind="value: TenKhuVuc" required="">
                </div>
            </div>
            <div class="form-group floatleft" data-bind="with: newNhomKhuVuc">
                <label class="form-label control-label ng-binding">Ghi chú </label>
                <div class="form-wrap">
                    <textarea rows="2" class=" ng-pristine ng-untouched ng-valid ng-empty" data-bind="value: GhiChu">        </textarea>
                </div>
            </div>
            <div class="form-group floatleft">
                <label class="form-label control-label req ng-binding">&nbsp;</label>
                <div class="footer-right pull-right">
                    <div class="modal-footer">
                        <button type="button" class="btn btn-cancel" data-dismiss="modal"><i class="fa fa-ban"></i> Đóng</button>
                        <button type="button" class="btn btn-delete " data-bind="click: modalDeleteKV"><i class="fa fa-trash"></i>Xóa</button>
                        <button type="button" class="btn btn-save" data-bind="click: $root.editNhomKhuVuc"><i class="fa fa-save"></i> Lưu</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
