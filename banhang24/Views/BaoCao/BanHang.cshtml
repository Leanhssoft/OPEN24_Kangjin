@using System.Web.Optimization
@{
    Layout = null;
    //ViewBag.Title = "Open24.vn - Báo Cáo Bán hàng";
}
<div class="container" >
    <div class="row">
        <div class="col-sm-12">
            <div class="row">
                <div class="op-filter">
                    <section class="op-filter-body">
                        <article class="boxLeft sortTime">
                            <h3 class="op-filter-title" onclick="toggleSubFilter(this)">
                                <img src="~/Content/images/icon/kieuhienthi.png" />     Kiểu hiển thị
                            </h3>
                            <span></span>
                            <aside class="op-filter-container">
                                <ul class="chose_kieubang">
                                    <li value="1">
                                        <a href="#home" data-toggle="tab">
                                            <input type="radio" name="kieubang" value="1" data-bind="checked: check_kieubang">
                                            <label>Biểu đồ</label>
                                        </a>
                                    </li>
                                    <li value="2">
                                        <a href="#info" data-toggle="tab">
                                            <input type="radio" name="kieubang" value="2" href="#info" data-toggle="tab" data-bind="checked: check_kieubang">
                                            <label>Báo cáo</label>
                                        </a>
                                    </li>
                                </ul>
                            </aside>
                        </article>
                        <article class="boxLeft sortTime">
                            <h3 class="op-filter-title" onclick="toggleSubFilter(this)">
                                <img src="~/Content/images/icon/Iconbanhanggg-03.png" />     Hóa Đơn
                            </h3>
                            <aside class="op-filter-container">
                                <input type="text" placeholder="Theo mã chứng từ" class="form-control" id="txtMaHH" data-bind="event: {keyup: SelectMaHH}">
                            </aside>
                        </article>
                        <article class="boxLeft">
                            <h3 class="op-filter-title" onclick="toggleSubFilter(this)">
                                <img src="~/Content/images/icon/loaihang.png" />         Loại hàng
                            </h3>
                            <aside class="op-filter-container">
                                <div class="menuCheckbox choose_LoaiHang">
                                    <ul>
                                        <li>
                                            <input value="1" type="checkbox" data-bind="checked: LoaiSP_HH"> Hàng hóa
                                        </li>
                                        <li>
                                            <input value="2" type="checkbox" data-bind="checked: LoaiSP_DV"> Dịch vụ
                                        </li>
                                    </ul>
                                </div>
                            </aside>
                        </article>
                        <article class="boxLeft showChiNhanh">
                            <h3 class="op-filter-title" onclick="toggleSubFilter(this)">
                                <img src="~/Content/images/icon/loaithuchi.png" />  Chi nhánh
                            </h3>
                            <aside class="op-filter-container">
                                <div class="outselect">
                                    <div class="nameDonVi choose-person floatleft " data-toggle="dropdown" id="choose_DonVi">
                                        <ul data-bind="foreach: MangChiNhanh">
                                            <li class="li-input-span">
                                                <span data-bind="text: TenDonVi">
                                                </span>
                                                <span data-bind="click: $parent.CloseDonVi">
                                                    <i class="fa fa-times"></i>
                                                </span>
                                            </li>
                                        </ul>
                                        <input type="text" class="dropdown nameDonVi" id="NoteNameDonVi" data-bind="event:{keyup: NoteNameDonVi}" placeholder="Chọn chi nhánh..." />
                                    </div>
                                    <div class="selec-person dropdown-menu" id="selec-all-DonVi">
                                        <ul data-bind="foreach: DonVis">
                                            <li  data-bind="attr:{id: ID}, click: $parent.SelectedDonVi">
                                                <span data-bind="text: TenDonVi "></span>
                                        </ul>
                                    </div>
                                </div>
                            </aside>
                        </article>
                        <article class="boxLeft">
                            <h3 class="op-filter-title" onclick="toggleSubFilter(this)">
                                <img src="~/Content/images/icon/thoigian.png" />      Thời gian
                            </h3>
                            <span></span>
                            <aside class="op-filter-container">
                                <div class="menuCheckbox choose_TimeReport">
                                    <div class="form-group floatleft">
                                        <div class="radio-menu">
                                            <input type="radio" name="rd_TimeReport" value="1" checked="checked">
                                        </div>
                                        <div class="conten-choose">
                                            <div class=" choose-date">
                                                <div class="dr_TimeReport" data-toggle="dropdown" aria-expanded="true">
                                                    <input type="text" class="dropdown form-control choose-date-show ip_TimeReport" placeholder="Hôm nay">
                                                </div>
                                                <div class="dropdown-menu choose_txtTime">
                                                    <div class="col-md-4 col-sm-4">
                                                        <h3>Theo ngày và tuần</h3>
                                                        <ul>
                                                            <li value="1" style="display: block;"><a href="javascript:void(0);" >Hôm nay</a></li>
                                                            <li value="2" style="display: block;"><a href="javascript:void(0);" >Hôm qua</a></li>
                                                            <li value="3" style="display: block;"><a href="javascript:void(0);" >Tuần này</a></li>
                                                            <li value="4" style="display: block;"><a href="javascript:void(0);" >Tuần trước</a></li>
                                                            <li value="5" style="display: block;"><a href="javascript:void(0);" >7 ngày qua</a></li>
                                                        </ul>
                                                    </div>
                                                    <div class="col-md-4 col-sm-4">
                                                        <h3>Theo tháng và quý</h3>
                                                        <ul>
                                                            <li value="6" style="display: block;"><a href="javascript:void(0);" >Tháng này</a></li>
                                                            <li value="7" style="display: block;"><a href="javascript:void(0);" >Tháng trước</a></li>
                                                            <li value="8" style="display: block;"><a href="javascript:void(0);" >30 ngày qua</a></li>
                                                            <li value="9" style="display: block;"><a href="javascript:void(0);" >Quý này</a></li>
                                                            <li value="10" style="display: block;"><a href="javascript:void(0);" >Quý trước</a></li>
                                                        </ul>
                                                    </div><div class="col-md-4 col-sm-4">
                                                        <h3>Theo năm</h3>
                                                        <ul>
                                                            <li value="11" class="ng-scope" style="display: block;"><a href="javascript:void(0);" >Năm này</a></li>
                                                            <li value="12" class="ng-scope" style="display: block;"><a href="javascript:void(0);" >Năm trước</a></li>
                                                            <li value="13" class="ng-scope" style="display: block;"><a href="javascript:void(0);" >Toàn thời gian</a></li>
                                                        </ul>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group floatleft">
                                        <div class="radio-menu">
                                            <input type="radio" name="rd_TimeReport" value="2">
                                        </div>
                                        <div class="conten-choose">
                                            <div class="floatleft form-wrap">
                                                <input type="text" class="form-control ip_DateReport newDateTime" name="daterange" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </aside>
                        </article>
                        <aside class="op-filter-container">
                            <h3 class="op-filter-title groupi">
                                <img src="~/Content/images/icon/Iconbanhanggg-02.png" />           Nhóm hàng
                            </h3>
                            <div class="seach-nhomhang">
                                <input type="text" class="seach-nhomhang" data-bind="event: {keyup: NoteNhomHang}" id="SeachNhomHang" autocomplete="off" placeholder="Tìm kiếm nhóm hàng" />
                                <i class="fa fa-search"></i>
                            </div>
                            <div id="tatcanhh" class="treename SelectALLNhomHang" data-bind="click: $root.SelectRepoert_NhomHangHoa">
                                <i class="fa fa-angle-right" aria-hidden="true"></i> Tất cả
                            </div>
                            <ul class="treeview floatleft SelectNhomHang" id="treeviewnhomhang" data-bind="foreach: NhomHangHoas">
                                <li class="ss-li">
                                    <div class="li-top li-oo" data-bind="click: $root.SelectRepoert_NhomHangHoa, attr:{id: ID}">
                                        <span class="treename" data-bind="text: TenNhomHangHoa"></span>
                                    </div>
                                    <span class="close-ul" onclick="ShowNhom($(this))" data-bind="visible: Childs.length >0 "><i class="fa fa-caret-right" aria-hidden="true"></i></span>
                                    <ul data-bind="foreach: Childs">
                                        <li>
                                            <div class="li-top li-oo" data-bind="click: $root.SelectRepoert_NhomHangHoa, attr:{id: ID}">
                                                <span data-bind="text: TenNhomHangHoa" class="li-top li-pp"></span>
                                                <a href="javascript:void(0)" data-bind="click: $root.editNhomHangHoa"><i class="fa fa-pencil-square-o"></i></a>
                                            </div>
                                            <span class="close-ul" onclick="ShowNhom($(this))" data-bind="visible: Child2s.length >0 "><i class="fa fa-caret-right" aria-hidden="true"></i></span>
                                            <ul data-bind="foreach: Child2s">
                                                <li>
                                                    <div class="li-top li-oo" data-bind="click: $root.SelectRepoert_NhomHangHoa, attr:{id: ID}">
                                                        <span data-bind="text: TenNhomHangHoa" class="li-top li-pp"></span>
                                                    </div>
                                                </li>
                                            </ul>
                                        </li>
                                    </ul>
                                </li>
                            </ul>
                            <div class="close-goods">
                                <a href="javascript:void(0)" class="add-goods"><em>Xem thêm </em><i class="fa fa-caret-down "></i></a>
                                <a href="javascript:void(0)" class="c-goods"><em>Thu gọn</em> <i class="fa fa-caret-down "></i></a>
                            </div>
                        </aside>
                    </section>
                    <div class="op-filter-footer">
                        <button type="button" class="kv2Btn">Hoàn tất</button>
                    </div>
                </div>
                <div class="content-table  "  id="myScrollspy" style="top: 80px;position: sticky;">
                    <a href="javascript:void(0)" class="search-fillter"  onclick="showFilter()" title="Hiển thị bộ lọc"><i class="fa fa-search" ></i></a>
                    <div class="table_h">
                    </div>
                    <div class="tab-content">
                        <div class="tab-pane" id="home">
                            <a href="javascript:void(0)" class="seach-fillter seach-fillter-bottom"><i class="fa fa-search" aria-hidden="true"></i></a>
                            <div class="BieuDo_Chart" id="chart"></div>
                        </div>
                        <div class="tab-pane" id="info">
                            <div class=" repost-bg floatleft" id="printablediv5">
                                <div class="repost" id="hedeartr">
                                    <div class="title-report-center">Báo cáo bán hàng chi tiết</div>
                                    <p>Ngày bán: <span data-bind="text: TodayBC"></span></p>
                                    <p>Chi nhánh: <span data-bind="text: TenChiNhanh"></span></p>
                                </div>
                                <div class="btnMenu pull-right" >
                                    <button type="button" class="btn btn-main dropdown-toggle xuatfile" data-bind="click: ExportExcel"> <i class="fa fa-file-excel-o"></i> Xuất file</button>
                                    <button class="btn btn-main btn-group" id="btnExport" name="btnExport"  onclick="printPage()"><i class="fa fa-print icon-print"></i>In</button>
                                    <div class="btn-dropdown">
                                        <button type="button" class="btn btn-main  dropdown-toggle" onclick="showCollumnFilter(this)" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                             <i class="fa fa-angle-double-down"></i>
                                        </button>
                                        <div class="dropdown-list drop-column">
                                            <ul>
                                                <li>
                                                    <div class="checkbox">
                                                        <label><input type="checkbox" class="checkbox " checked="checked" value="0" id="bhmact"><span></span>Mã chứng từ</label>
                                                    </div>
                                                </li>
                                                <li>
                                                    <div class="checkbox">
                                                        <label><input type="checkbox" class="checkbox " checked="checked" value="1" id="bhtime"><span></span>Thời gian</label>
                                                    </div>
                                                </li>
                                                <li>
                                                    <div class="checkbox">
                                                        <label><input type="checkbox" class="checkbox " checked="checked" value="2" id="bhmahh"><span></span>Mã hàng hóa</label>
                                                    </div>
                                                </li>
                                                <li>
                                                    <div class="checkbox">
                                                        <label><input type="checkbox" class="checkbox " checked="checked" value="3" id="bhname"><span></span>Tên hàng hóa</label>
                                                    </div>
                                                </li>
                                                <li>
                                                    <div class="checkbox">
                                                        <label><input type="checkbox" class="checkbox " checked="checked" value="4" id="bhsoluong"><span></span>Số lượng</label>
                                                    </div>
                                                </li>
                                                <li>
                                                    <div class="checkbox">
                                                        <label><input type="checkbox" class="checkbox " checked="checked" value="5" id="bhgiaban"><span></span>Giá bán</label>
                                                    </div>
                                                </li>
                                                <li>
                                                    <div class="checkbox">
                                                        <label><input type="checkbox" class="checkbox " checked="checked" value="6" id="bhchietkhau"><span></span>Chiết khấu</label>
                                                    </div>
                                                </li>
                                                <li>
                                                    <div class="checkbox">
                                                        <label><input type="checkbox" class="checkbox " checked="checked" value="7" id="bhthanhtien"><span></span>Thành tiền</label>
                                                    </div>
                                                </li>
                                                <li>
                                                    <div class="checkbox">
                                                        <label><input type="checkbox" class="checkbox " checked="checked" value="8" id="bhgiavon"><span></span>Giá vốn</label>
                                                    </div>
                                                </li>
                                                <li>
                                                    <div class="checkbox">
                                                        <label><input type="checkbox" class="checkbox " checked="checked" value="9" id="bhtienvon"><span></span>Tiền vốn</label>
                                                    </div>
                                                </li>
                                                <li>
                                                    <div class="checkbox">
                                                        <label><input type="checkbox" class="checkbox " checked="checked" value="10" id="bhgiamgia"><span></span>Giảm giá HD</label>
                                                    </div>
                                                </li>
                                                <li>
                                                    <div class="checkbox">
                                                        <label><input type="checkbox" class="checkbox " checked="checked" value="11" id="bhlailo"><span></span>Lãi lỗ</label>
                                                    </div>
                                                </li>
                                                <li>
                                                    <div class="checkbox">
                                                        <label><input type="checkbox" class="checkbox " checked="checked" value="12" id="bhnhanvien"><span></span>Nhân viên</label>
                                                    </div>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                                <div class="table-reponsive" id="table-reponsive">
                                    <table class="table table-striped table-hover">
                                        <thead>
                                            <tr>
                                                <th class="bhmact text-center">Mã chứng từ</th>
                                                <th class="bhtime text-center">Thời gian</th>
                                                <th class="bhmahh text-center">Mã hàng hóa</th>
                                                <th class="bhname text-left">Tên hàng hóa</th>
                                                <th class="bhsoluong text-right">Số lượng</th>
                                                <th class="bhgiaban text-right">Giá bán</th>
                                                <th class="bhchietkhau text-right">Chiết khấu</th>
                                                <th class="bhthanhtien text-right">Thành tiền</th>
                                                <th class="bhgiavon text-right">Giá vốn</th>
                                                <th class="bhtienvon text-right">Tiền vốn</th>
                                                <th class="bhgiamgia text-right">Giảm giá HD</th>
                                                <th class="bhlailo text-right">Lãi lỗ</th>
                                                <th class="bhnhanvien text-center">Nhân viên</th>
                                            </tr>
                                        </thead>
                                        <tbody data-bind="foreach: ReportHH_BanHang">
                                            <tr class="prev-tr-hide">
                                                <td class="bhmact cls1 text-center" data-bind="text: MaHoaDon"></td>
                                                <td class="bhtime cls2 text-center">
                                                    <span data-bind="text: moment(NgayLapHoaDon).format('DD/MM/YYYY')!='Invalid date'? moment(NgayLapHoaDon).format('DD/MM/YYYY HH:mm'):''"></span>
                                                </td>
                                                <td class="bhmahh cls1 text-center" data-bind="text: MaHangHoa"></td>
                                                <td class="bhname cls3 text-left">
                                                    <span data-bind="text: TenHangHoa"></span>
                                                    <span data-bind="text: ThuocTinh_GiaTri" style="color:#ff6a00"></span>
                                                    <span data-bind="text: TenDonViTinh" style="color:#0094ff"></span>
                                                    <span data-bind="text: TenLoHang" style="color:#000000"></span>
                                                </td>
                                                <td class="bhsoluong cls4 text-right" data-bind="text: formatNumber(SoLuong) "></td>
                                                <td class="bhgiaban cls5 text-right " data-bind="text: formatNumber(GiaBan)"></td>
                                                <td class="bhchietkhau cls5 text-right" data-bind="text: formatNumber(TienChietKhau)"></td>
                                                <td class="bhthanhtien cls6 text-right" data-bind="text: formatNumber(ThanhTien) "></td>
                                                <td class="bhgiavon cls7 text-right" data-bind="text: formatNumber(GiaVon) "></td>
                                                <td class="bhtienvon cls8 text-right" data-bind="text: formatNumber(TienVon) "></td>
                                                <td class="bhgiamgia cls5 text-right" data-bind="text: formatNumber(GiamGiaHD)"></td>
                                                <td class="bhlailo cls9 text-right" data-bind="text: formatNumber(LaiLo) "></td>
                                                <td class="bhnhanvien cls10 text-center" data-bind="text: TenNhanVien"></td>
                                            </tr>
                                        </tbody>
                                        <tfoot class="PhanQuyen" hidden>
                                            <tr>
                                                <td class="bg12 bhma text-center" colspan="13"><i>Bạn không có quyền xem báo cáo này!</i></td>
                                            </tr>
                                        </tfoot>
                                        <tfoot class="Report_Empty" hidden>
                                            <tr>
                                                <td class="bg12 bhma text-center" colspan="13"><i>Báo cáo không có dữ liệu</i></td>
                                            </tr>
                                        </tfoot>
                                        <tfoot id="TongCongBanHang">
                                            <tr>
                                                <td class="bg12 text-left bold bhmact">Tổng cộng</td>
                                                <td class="bg12 bhtime"></td>
                                                <td class="bg12 bhmahh"></td>
                                                <td class="bg12 bhname"></td>
                                                <td class="bg12 bhsoluong text-right bold" data-bind="text: formatNumber(_SoLuong())"></td>
                                                <td class="bg12 bhgiaban"></td>
                                                <td class="bg12 bhchietkhau"></td>
                                                <td data-bind="text: formatNumber(_ThanhTien())" class="bg12 text-right bold bhthanhtien"></td>
                                                <td class=" bg12 bhgiavon"></td>
                                                <td data-bind="text: formatNumber(_TienVon())" class="bg12 text-right bold bhtienvon"></td>
                                                <td class="bg12 bhgiamgia"></td>
                                                <td data-bind="text: HangHoa_XemGiaVon() == 'HangHoa_XemGiaVon'? formatNumber(_LaiLo()): '0'" class="bg12 border-bottom text-right bold bhlailo"></td>
                                                <td class="bg12 bhnhanvien"></td>
                                            </tr>
                                        </tfoot>
                                    </table>
                                    @*Phân trang báo cáo*@
                                </div>
                                <div class="page" hidden>
                                    <div class="flex flex-end">
                                        <a href="javascript:void(0)" data-bind="click: $root.StartPage" id="StartPage"><i class="fa fa-step-backward" aria-hidden="true"></i></a>
                                        <a href="javascript:void(0)" data-bind="click: $root.BackPage" id="BackPage"><i class="fa fa-caret-left" aria-hidden="true"></i></a>
                                        <ul data-bind="foreach: SumNumberPageReport">
                                            <li data-bind="click: $parent.gotoNextPage">
                                                <a href="javascript:void(0)" data-bind="text: SoTrang,css: $root.GetClass($data)"></a>
                                            </li>
                                        </ul>
                                        <a href="javascript:void(0)" data-bind="click: $root.NextPage" id="NextPage"><i class="fa fa-caret-right" aria-hidden="true"></i></a>
                                        <a href="javascript:void(0)" data-bind="click: $root.EndPage" id="EndPage"><i class="fa fa-step-forward" aria-hidden="true"></i></a>
                                        <div class="total-recos">
                                            Hiển thị <span data-bind="text: RowsStart"></span> - <span data-bind="text: RowsEnd"></span> trên tổng số
                                            <span data-bind="text: SumRowsHangHoa"></span> hóa đơn
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="tab-pane" id="info">
                            <div class=" repost-bg floatleft" id="printtable">
                                <div class="repost" id="hedearprinttr">
                                    <div class="title-report-center tieudetr">Báo cáo bán hàng chi tiết</div>
                                    <p>Ngày bán: <span data-bind="text: TodayBC"></span></p>
                                    <p class="nameChiNhanhTr">Chi nhánh: <span data-bind="text: TenChiNhanh"></span></p>
                                </div>
                                <div id="table-reponsivetr">
                                    <table class="table table-striped table-hover">
                                        <thead>
                                            <tr>
                                                <th class="bhmact itemCenter">Mã chứng từ</th>
                                                <th class="bhtime itemCenter">Thời gian</th>
                                                <th class="bhmahh itemCenter">Mã hàng hóa</th>
                                                <th class="bhname itemLeft">Tên hàng hóa</th>
                                                <th class="bhsoluong itemRight" style="padding-right: 9px;">Số lượng</th>
                                                <th class="bhgiaban itemRight" style="padding-right: 9px;">Giá bán</th>
                                                <th class="bhchietkhau itemRight" style="padding-right: 9px;">Chiết khấu</th>
                                                <th class="bhthanhtien itemRight" style="padding-right: 9px;">Thành tiền</th>
                                                <th class="bhgiavon itemRight" style="padding-right: 9px;">Giá vốn</th>
                                                <th class="bhtienvon itemRight" style="padding-right: 9px;">Tiền vốn</th>
                                                <th class="bhgiamgia itemRight" style="padding-right: 9px;">Giảm giá HD</th>
                                                <th class="bhlailo itemRight" style="padding-right: 9px;">Lãi lỗ</th>
                                                <th class="bhnhanvien itemCenter">Nhân viên</th>
                                            </tr>
                                        </thead>
                                        <tbody data-bind="foreach: ReportHH_BanHangPrint">
                                            <tr class="prev-tr-hide">
                                                <td class="bhmact cls1 itemCenter" data-bind="text: MaHoaDon"></td>
                                                <td class="bhtime cls2 itemCenter">
                                                    <span data-bind="text: moment(NgayLapHoaDon).format('DD/MM/YYYY')!='Invalid date'? moment(NgayLapHoaDon).format('DD/MM/YYYY HH:mm'):''"></span>
                                                </td>
                                                <td class="bhmahh cls1 itemCenter" data-bind="text: MaHangHoa"></td>
                                                <td class="bhname cls3 itemLeft">
                                                    <span data-bind="text: TenHangHoa"></span>
                                                    <span data-bind="text: ThuocTinh_GiaTri" style="color:#ff6a00"></span>
                                                    <span data-bind="text: TenDonViTinh" style="color:#0094ff"></span>
                                                    <span data-bind="text: TenLoHang" style="color:#000000"></span>
                                                </td>
                                                <td class="bhsoluong cls4 itemRight" style="padding-right: 9px;" data-bind="text: formatNumber(SoLuong) "></td>
                                                <td class="bhgiaban cls5 itemRight" style="padding-right: 9px;" data-bind="text: formatNumber(GiaBan)"  ></td>
                                                <td class="bhchietkhau cls5 itemRight" style="padding-right: 9px;" data-bind="text: formatNumber(TienChietKhau)" ></td>
                                                <td class="bhthanhtien cls6 itemRight" style="padding-right: 9px;" data-bind="text: formatNumber(ThanhTien) "  ></td>
