<style>
    .title-col-2 label {
        padding-left: 0px !important;
    }
   
</style>
<div class="modal-content  ">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><i class="material-icons">close</i></button>

        <h4 class="modal-title" id="lblTitle"></h4>
    </div>
    <div class="modal-body">
        <div class="col-md-9 col-sm-12 col-xs-12 nopadding-left">
            <div class="form-group floatleft">
                <div class="col-md-3 col-sm-3 col-xs-12 nopadding"><label>Đối tượng</label> </div>
                <div class="col-md-9 col-sm-9 col-xs-12">
                    <div class="input-group">
                        <p class="nopadding no-magrin">
                            <label>
                                <input type="radio" name="ko_unique"
                                       data-bind="checked: IsCustomer, checkedValue: true"> Khách hàng
                            </label>
                            <label>
                                <input type="radio" name="ko_unique"
                                       data-bind="checked: IsCustomer, checkedValue: false"> Nhà cung cấp
                            </label>
                        </p>
                    </div>
                </div>
            </div>
            <div class="form-group floatleft">
                <div class="col-md-3 nopadding">
                    <label>Mã liên hệ</label>
                </div>
                <div class="col-xs-12 col-sm-12 col-md-9 nopadding">
                    <input type="text" class="form-control" placeholder="Mã tự động" data-bind="value: newUserContact().MaLienHe">
                </div>
            </div>
            <div class="form-group floatleft">
                <div class="col-md-3 nopadding">
                    <label>Tên người liên hệ</label>
                </div>
                <div class="col-xs-12 col-sm-12 col-md-9 nopadding">
                        <input type="text" class="form-control" data-bind="value: newUserContact().TenLienHe" autofocus style="width:calc(100% - 115px); display:inline-block">
                        <div  style=" display:inline-block; width:110px">
                            <select class="form-control" data-bind="options: ListXungHo,
                                           optionsText: 'Text',
                                           optionsValue: 'ID',
                                           value: newUserContact().XungHo,
                                           "></select>
                        </div>
                </div>
            </div>
            <div class="form-group floatleft">
                <div class="col-md-3 nopadding">
                    <label id="lblDoiTuong">Khách hàng</label>
                </div>
                <div class="col-xs-12 col-sm-12 col-md-9 nopadding">
                    <div class="form-group from-tt-qh">
                        <div class="form-control input-combobox floatleft collection" id="txtCustomer_modal">--Chọn khách hàng--</div>
                        <button class="btn-combobox"> <i class="fa fa-caret-down"></i></button>
                        <div id="showseach_DoiTuong" hidden style="width:260px !important" class="divSearchVue row-combobox">
                            <div class="search-result-combobox">
                                <input type="text" class="form-control" placeholder="Nhập tìm kiếm"
                                       data-bind="value:filterDoiTuong ,valueUpdate: 'afterkeydown'" />
                            </div>
                            <ul>
                                <li data-bind="click: ChoseCustomer_Modal">
                                    <a style="font-weight: bold;">
                                        <span>--Chọn khách hàng--</span>
                                    </a>
                                </li>
                                <!--ko foreach: FilterDoiTuong -->
                                <li data-bind="click: $parent.ChoseCustomer_Modal">
                                    <a style="font-weight: bold;">
                                        Tên:
                                        <span data-bind="text:TenDoiTuong"></span>
                                        <span data-bind="attr:{id: 'spanCheckDoiTuong_'+ ID}" class="pull-right"></span><br />
                                        Mã: <span data-bind="text: MaDoiTuong"></span>
                                        <span data-bind="visible: DienThoai">
                                            - ĐT: <span data-bind="text: DienThoai"></span>
                                        </span>
                                    </a>
                                </li>
                                <!--/ko-->
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="form-group floatleft">
                <div class="col-md-3 nopadding">
                    <label>Di động</label>
                </div>
                <div class="col-xs-12 col-sm-12 col-md-9  nopadding">
                    <input type="text" class="form-control" onkeypress="return chekPhone(event)"
                           data-bind="value: newUserContact().SoDienThoai">
                </div>
            </div>
            <div class="form-group floatleft">
                <div class="col-md-3 nopadding">
                    <label>Điện thoại cố định</label>
                </div>
                <div class="col-xs-12 col-sm-12 col-md-9 nopadding">
                    <input type="text" class="form-control" onkeypress="return chekPhone(event)"
                           data-bind="value: newUserContact().DienThoaiCoDinh">
                </div>
            </div>
            <div class="form-group floatleft">
                <div class="col-md-3 nopadding">
                    <label>Sinh nhật</label>
                </div>
                <div class="col-xs-12 col-sm-12 col-md-9  nopadding">
                    <input type="text" class="form-control datepicker_mask"
                           data-bind="value: newUserContact().NgaySinh, click: formatDateTime">
                </div>
            </div>
            <div class="form-group floatleft">
                <div class="col-md-3 nopadding ">
                    <label>Email</label>
                </div>
                <div class="col-xs-12 col-sm-12 col-md-9  nopadding">
                    <input type="text" class="form-control" data-bind="value: newUserContact().Email">
                </div>
            </div>
            <div class="form-group floatleft">
                <div class="col-md-3 nopadding ">
                    <label>Chức vụ</label>
                </div>
                <div class="col-xs-12 col-sm-12 col-md-9  nopadding">
                    <input type="text" class="form-control" data-bind="value: newUserContact().ChucVu">
                </div>
            </div>
            <div class="form-group floatleft">
                <div class="col-md-3 nopadding">
                    <label>Tỉnh thành</label>
                </div>
                <div class="col-xs-12 col-sm-12 col-md-9 nopadding">
                    <div class="form-group from-tt-qh">
                        <div class="form-control input-combobox floatleft collection" id="txtProvince_modal">--Chọn Tỉnh thành--</div>
                        <button class="btn-combobox"> <i class="fa fa-caret-down"></i></button>
                        <div id="showseach_Pr" hidden style="width:183px !important" class="divSearchVue row-combobox">
                            <div class="search-result-combobox">
                                <input type="text" class="form-control" placeholder="Nhập tìm kiếm" data-bind="value:searchTT ,valueUpdate: 'afterkeydown'" />
                            </div>
                            <ul>
                                <li data-bind="click: ChoseProvince_Modal">
                                    <a style="font-weight: bold;">
                                        <span>--Chọn Tỉnh thành--</span>
                                    </a>
                                </li>
                                <!--ko foreach: listProvinces -->
                                <li data-bind="click: $parent.ChoseProvince_Modal">
                                    <a style="font-weight: bold;">
                                        <span data-bind="text:TenTinhThanh"></span>
                                    </a>
                                </li>
                                <!--/ko-->
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="form-group floatleft">
                <div class="col-md-3  nopadding">
                    <label>Quận huyện</label>
                </div>
                <div class="col-xs-12 col-sm-12 col-md-9  nopadding">
                    <div class="form-group from-tt-qh" id="divSearch_Dt">
                        <div class="form-control input-combobox floatleft collection " id="txtDistrict_modal">--Chọn quận huyện--</div>
                        <button class="btn-combobox"> <i class="fa fa-caret-down"></i></button>
                        <div id="showseach_Dt" hidden style="width:183px !important" class="divSearchVue row-combobox">
                            <div class="search-result-combobox">
                                <input type="text" class="form-control" placeholder="Nhập tìm kiếm"
                                       data-bind="value:searchDistrict ,valueUpdate: 'afterkeydown'" />
                            </div>
                            <ul>
                                <li data-bind="click: ChoseProvince_Modal">
                                    <a style="font-weight: bold;">
                                        <span>--Chọn quận huyện--</span>
                                    </a>
                                </li>
                                <!--ko foreach: FilterDistrict_byProvice -->
                                <li data-bind="click: $parent.ChoseDistrict_Modal">
                                    <a style="font-weight: bold;">
                                        <span data-bind="text:TenQuanHuyen"></span>
                                    </a>
                                </li>
                                <!--/ko-->
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="form-group floatleft">
                <div class="col-md-2 nopadding title-col-2">
                    <label><strong>Địa chỉ</strong></label>
                </div>
                <div class=" col-md-10" style="padding-left:25px;padding-right:0;">
                    <input type="text" class="form-control" data-bind="value: newUserContact().DiaChi">
                </div>
            </div>
            <div class="form-group floatleft">
                <div class="col-md-2 nopadding title-col-2">
                    <label><strong>Ghi chú</strong></label>
                </div>
                <div class=" col-md-10" style="padding-left:25px;padding-right:0;">
                    <textarea style="width:100%;" rows="1" type="text" data-bind="value: newUserContact().GhiChu" />
                </div>
            </div>
        </div>
        <div class="col-md-3 nopadding col-sm-12 col-xs-12">
            <div class="img-user-contact floatleft" style="float:left">
                <!--  ko if: HaveImage_Select() == false -->
                <img src="~/Content/images/photo.png"
                     style="border: 1px dotted #ccc; margin-left:5px;width:99%; height: 99%;" />
                <!--/ko-->
                <ul data-bind="foreach: FilesSelect ">
                    <!-- ko if: $index() == 0-->
                    <li>
                        <div class="img-customer" style="margin-right: calc(50% - 120px); width:100%;height: 150px ; min-width:150px;">
                            <img alt="" data-bind="attr: {src: URLAnh }" />
                            <a data-bind="click: $root.DeleteImg">
                                <i class="fa fa-close red"></i>
                            </a>
                        </div>
                    </li>
                    <!--/ko-->
                </ul>
            </div>
            <div class="choose-img" style="padding-left:5px; width: 100%;">
                <span class="btn  btn-file form-control" data-bind="event:{change: $root.fileSelect}" style="margin-top:5px;">
                    Chọn ảnh <input type='file' multiple id="imageUpHHForm" name="files[]" accept=".png,.jpg,.jpeg,.gif" style="width:100%"/>
                    <output id="list"></output>
                </span>
            </div>
        </div>
    </div>
    <div class="modal-footer">
        <div class="container-fluid">
            <button type="button" class="btn btn-cancel" data-dismiss="modal"><i class="fa fa-ban"></i>&nbsp;Bỏ qua</button>
            <button type="button" class="btn btn-save" data-bind="click: addUpdate_UserContract">
                <i class="fa fa-save"></i>&nbsp; Lưu
            </button>
        </div>
    </div>
    
</div>
<script>
    console.log(1);
    $('.from-tt-qh').on('click', '.btn-combobox', function () {
        $(".search-result-combobox,.search-result-combobox input").mouseup(function () {
            return false;
        });
        var display = $(this).next('.divSearchVue').css('display');
        $('.divSearchVue').hide();
        if (display === 'none')
            $(this).next('.divSearchVue').toggle();
    });
    $('.from-tt-qh').on('click', '.input-combobox', function () {
        $(".search-result-combobox,.search-result-combobox input").mouseup(function () {
            return false;
        });
        var display = $(this).next().next('.divSearchVue').css('display');
        $('.divSearchVue').hide();
        if (display === 'none')
            $(this).next().next('.divSearchVue').toggle();
    });
    $(".input-combobox,.btn-combobox").mouseup(function () {
        return false;
    });
    $(document).mouseup(function () {
        $('.divSearchVue').hide();
    });
</script>