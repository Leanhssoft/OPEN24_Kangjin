@using System.Web.Optimization
@using banhang24.Hellper;
@using Model;
@model RoleChamCongModel
@{
    Layout = null;
}
@Html.Hidden("RoleChamCong", Model.ChamCong?1:0, new { @id = "ID_RoleChamCong" })
<link href="~/Content/NhanSu.css" rel="stylesheet" />
<div class="op-container" id="ChamCong">
    <div class="container-fluid nopadding">
        <div class="col-left">
            <div class="op-filter">
                <section class="op-filter-body">
                    <article class="boxLeft">
                        <h3 class="op-filter-title" v-on:click="ClickFilter.BangCong=!ClickFilter.BangCong">
                            <img src="/Content/images/icon/report-loaibaocao.png">Bảng công
                        </h3>
                        <aside class="op-filter-container" v-show="ClickFilter.BangCong">
                            <ul class=" chose_kieubang ul-radius">
                                <li v-bind:class="[LoaiCong===1 ? 'active' : '']">
                                    <a href="javascript:void(0)" v-bind:class="[LoaiCong===1 ? 'box-tab' : '']" v-on:click="ChangeLoaiCong(1)">
                                        <input type="radio" name="kieubang" data-toggle="tab">
                                        <label>Chấm công</label>
                                    </a>
                                </li>
                                <li v-bind:class="[LoaiCong===2 ? 'active' : '']">
                                    <a href="javascript:void(0)" v-on:click="ChangeLoaiCong(2)" v-bind:class="[LoaiCong===2 ? 'box-tab' : '']">
                                        <input type="radio" name="kieubang" data-toggle="tab">
                                        <label>Bảng công</label>
                                    </a>
                                </li>
                            </ul>
                        </aside>
                    </article>
                    <article class="boxLeft">
                        <h3 class="op-filter-title" onclick="toggleSubFilter(this)" v-on:click="ClickFilter.KyTinhCong=!ClickFilter.KyTinhCong">
                            <img src="~/Content/images/icon/thoi-gian.png" height="14" />
