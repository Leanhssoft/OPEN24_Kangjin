<script>
    $(document).ready(function () {
        $(window.document).on('shown.bs.modal', '.modal', function () {
            window.setTimeout(function () {
                $('[autofocus]', this).focus();
                $('[autofocus]').select();
            }.bind(this), 100);
            $('.datepicker_mask').datetimepicker({
                timepicker: false,
                mask: false,
                format: 'd/m/Y H:i',
            });
            $(document).keydown(function (e) {
                if (e.keyCode == 9) {
                    $('.datepicker_mask').datetimepicker({
                        timepicker: false,
                        mask: true,
                        format: 'd/m/Y H:i',
                    });
                }
            });
        });
        $('.modal-dialog').draggable({
            handle: ".modal-header"
        });
    });
</script>