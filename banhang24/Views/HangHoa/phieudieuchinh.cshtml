                                    <li>
                                        <div class="checkbox">
                                            <label><input type="checkbox" class="checkbox" id="cbMaPhieu" checked="checked" value="0" /><span></span>Mã phiếu</label>
                                        </div>
                                    </li>
                                    <li>
                                        <div class="checkbox">
                                            <label><input type="checkbox" class="checkbox" id="cbThoiGian" checked="checked" value="1" /><span></span>Thời gian</label>
                                        </div>
                                    </li>
                                    <li>
                                        <div class="checkbox">
                                            <label><input type="checkbox" class="checkbox" id="cbTenDonVi" checked="checked" value="2" /><span></span>Tên đơn vị</label>
                                        </div>
                                    </li>
                                    <li>
                                        <div class="checkbox">
                                            <label><input type="checkbox" class="checkbox" id="cbGhiChu" checked="checked" value="3" /><span></span>Ghi chú</label>
                                        </div>
                                    </li>
                                    <li>
