                        if (check == 1) {
                            ShowMessage_Danger('Vui lòng chọn xe để thực hiện bảo dưỡng');
                            return false;
                        }
                        if (check == 2) {
                            ShowMessage_Danger('Xe ' + gdv.BienSo + ' hiện tại không có trong xưởng');
