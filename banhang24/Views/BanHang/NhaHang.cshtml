@using System.Web.Optimization
@using banhang24.AppCache
@using banhang24.Hellper
@{
    Layout = null;
}
<style>
    .myddl {
        border: 1px solid #ccc;
        border-radius: 1px !important;
    }
    .loading-box {
        position: fixed;
        top: 0;
        left: 0;
        height: 100vh;
        width: 100vw;
        z-index: 999999999999999999
    }
    .loading-bg {
        background: #fff;
        height: 100%
    }
    .loading-obj {
        overflow: visible;
        position: relative;
        background: #fff;
        width: 200px;
        height: 200px;
        margin: auto;
        top: calc(50vh - 200px);
        border-radius: 50%
    }
    .loading-img {
        margin: 82px 50px;
        vertical-align: middle
    }
    .loading-txt {
        width: 200px;
        margin: auto;
        padding-left: 100px
    }
    .loading-done {
        display: none;
        position: absolute;
        top: 0;
        background: #fff;
        width: 200px;
        height: 200px;
        border-radius: 50%;
        text-align: center;
        padding: 50px
    }
    .loader {
        border: 5px solid var(--color-primary);
        border-radius: 50%;
        border-top: 5px solid #fff;
        border-left: 5px solid #fff;
        border-bottom: 5px solid #fff;
        height: 120px;
        top: 15px;
        right: 15px;
        z-index: 999;
        animation: spin 1s cubic-bezier(.4, 0, .2, 1) infinite;
        position: absolute;
        width: 150px;
        height: 150px
    }
</style>
