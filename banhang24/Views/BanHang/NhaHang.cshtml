    $("#charge-box").on("click", function () {
        $(".arrow-left.arrow-left2").hide();
        $(this).next().show();
        $("#charge-box").mouseup(function () {
            return false;
        })
        $(document).mouseup(function () {
            $("#charge-box").next().hide();
        })
    }
    )
</script>
<script>
    //điều hướng menu di dộng/ tablet
    $("#redirect_divLeft").click(function () {
        $(this).parent().removeClass("act1");
        $(this).parent().removeClass("act2");
        $(this).parent().addClass("act0");
        $(this).addClass("active");
        $(this).siblings().removeClass("active");
        document.getElementById("header_left").scrollIntoView();
    });
    $("#redirect_divRight").click(function () {
        $(this).parent().removeClass("act0");
        $(this).parent().removeClass("act2");
        $(this).parent().addClass("act1");
        $(this).addClass("active");
        $(this).siblings().removeClass("active");
        $("#tt_tab").removeClass("active");
        $("#td_tab").addClass("active");
        document.getElementById("content_right").scrollIntoView();
    });
    $("#redirect_mr").click(function () {
        $(this).parent().removeClass("act0");
        $(this).parent().removeClass("act1");
        $(this).parent().addClass("act2");
        $(this).addClass("active");
        $(this).siblings().removeClass("active");
        $("#tt_tab").addClass("active");
        $("#td_tab").removeClass("active");
        document.getElementById("content_right").scrollIntoView();
    });
    $("#td_tab").click(function () {
        $(".mobile-nav").removeClass("act0");
        $(".mobile-nav").removeClass("act2");
        $(".mobile-nav").addClass("act1");
        $("#redirect_divRight").siblings().removeClass("active");
        $("#redirect_divRight").addClass("active");
    });
    $("#tt_tab").click(function () {
        $(".mobile-nav").removeClass("act0");
        $(".mobile-nav").removeClass("act1");
        $(".mobile-nav").addClass("act2");
        $("#redirect_mr").siblings().removeClass("active");
        $("#redirect_mr").addClass("active");
    });
</script>
<script>
    $(".hide-btn").on("click", function () {
        $(".infor-TT").addClass("hiden-md");
        $("#th").addClass("show-md");
        $("#tt").addClass("show-md");
        $("#mr").addClass("show-md");
        $(this).hide();
        $(".show-btn").show();
    })
    $(".show-btn").on("click", function () {
        $(".infor-TT").removeClass("hiden-md");
        $("#th").removeClass("show-md");
        $("#tt").removeClass("show-md");
        $("#mr").removeClass("show-md");
        $(this).hide();
        $(".hide-btn").show();
    });
    $.datetimepicker.setLocale('vi');
    $(function () {
        $('#txtNgayTaoHD').datetimepicker({
            format: "d/m/Y H:i",
            defaultDate: new Date(),
            mask: true,
            maxDate: new Date(),
            scrollMonth: false,
        })
    });
    function openPopup(ele) {
        $(ele).next().toggle();
    }
    $(document).mouseup(function (e) {
        var container = $(".popup-timer");
        if (!container.is(e.target) && container.has(e.target).length === 0) {
            container.hide();
        }
    });
</script>
