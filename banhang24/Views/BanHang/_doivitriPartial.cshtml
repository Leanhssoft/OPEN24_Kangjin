<div class="modal-content  ui-draggable">
    <div class="modal-header ui-draggable-handle">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><i class="material-icons">close</i></button>

        <h4 class="modal-title">Đổi vị trí</h4>
    </div>
    <div class="modal-body floatleft" style="height: 80vh !important;">
        <div class="paddingb30">
            <div class="menu-room">
                <ul class="nav @*nav-tabs*@ tab-location list-inline " style="width:100% !important">
                    <li class="active" data-bind="click: GetTableEmpty_ByIDNhom">
                        <a data-toggle="tab" href="#popallLocation" aria-expanded="true">Tất cả</a>
                    </li>
                    <!--ko foreach: AllNhomBans -->
                    <li data-bind="click: $root.GetTableEmpty_ByIDNhom">
                        <a data-toggle="tab" aria-expanded="true" data-bind="text: TenKhuVuc"></a>
                    </li>
                    <!--/ko-->
                </ul>
                <div class="tab-content">
                    <div class="tab-pane active" id="popallLocation">
                        <ul class="list-pop-room" data-bind="foreach: TableEmpty_ByIDNhom">
                            <li data-bind="click: $root.Popup_ChoseTable">
                                <a href="javascript:void(0)">
                                    <strong data-bind="text: TenViTri, css:{'room-emty': $root.IDPhongBan_Chosing() !== $data.ID }"></strong>
                                    &nbsp;(<span data-bind="text: TenKhuVuc"></span>)
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <div class="modal-footer" hidden>
           
                <button type="button" class="btn btn-save" data-dismiss="modal"><i class="fa fa-ban"></i> Bỏ qua</button>
                <button type="button" class="btn btn-cancel" data-bind="click: SaveVitri_Chosed"><i class="fa fa-floppy-o" aria-hidden="true"></i> Lưu</button>
           
        </div>
    </div>
</div>