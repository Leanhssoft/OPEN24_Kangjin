<div class="modal-content ">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><i class="material-icons">close</i></button>

        <h4 class="modal-title">
            Ẩn hiện cột trên hóa đơn
        </h4>
    </div>
    <div class="modal-body floatleft">
       
                <div class="form-group floatleft">
                    <label class="form-label  control-label req ng-binding checkhanghoaxoa">
                        Số thứ tự
                    </label>
                    <div class="show-table">
                        <span class="main-show" id="divSet_showSTT" data-bind="click: function(){Setting_HideShowColumCTHD(event.currentTarget.id)}">
                            <span class="turnon">Bật</span>
                            <span class="turnoff">Tắt</span>
                        </span>
                    </div>
                </div>
                <div class="form-group floatleft">
                    <label class="form-label  control-label req ng-binding checkhanghoaxoa">
                        Mã hàng
                    </label>
                    <div class="show-table">
                        <span class="main-show" id="divSet_showProductCode" data-bind="click: function(){Setting_HideShowColumCTHD(event.currentTarget.id)}">
                            <span class="turnon">Bật</span>
                            <span class="turnoff">Tắt</span>
                        </span>
                    </div>
                </div>


                <div class="form-group floatleft">
                    <label class="form-label  control-label req ng-binding checkhanghoaxoa">
                        Giá bán
                    </label>
                    <div class="show-table">
                        <span class="main-show" id="divSet_showProductPrice" data-bind="click: function(){Setting_HideShowColumCTHD(event.currentTarget.id)}">
                            <span class="turnon">Bật</span>
                            <span class="turnoff">Tắt</span>
                        </span>
                    </div>
                </div>
                <div class="form-group floatleft">
                    <label class="form-label  control-label req ng-binding checkhanghoaxoa">
                        Chiết khấu
                    </label>
                    <div class="show-table">
                        <span class="main-show" id="divSet_showProductSale" data-bind="click: function(){Setting_HideShowColumCTHD(event.currentTarget.id)}">
                            <span class="turnon">Bật</span>
                            <span class="turnoff">Tắt</span>
                        </span>
                    </div>
                </div>

                <div class="form-group floatleft">
                    <label class="form-label  control-label req ng-binding checkhanghoaxoa">
                        Thành tiền
                    </label>
                    <div class="show-table">
                        <span class="main-show" id="divSet_showSumPrice" data-bind="click: function(){Setting_HideShowColumCTHD(event.currentTarget.id)}">
                            <span class="turnon">Bật</span>
                            <span class="turnoff">Tắt</span>
                        </span>
                    </div>
                </div>
                <div class="form-group floatleft" hidden>
                    <label class="form-label  control-label req ng-binding checkhanghoaxoa">
                        Thêm dòng
                    </label>
                    <div class="show-table">
                        <span class="main-show">
                            <span class="turnon">Bật</span>
                            <span class="turnoff">Tắt</span>
                        </span>
                    </div>
                </div>

           
    </div>
    <div class="modal-footer">
      
            <button type="button" class="btn btn-main" data-bind="click: Agree_SettingHideShowColumCTHD">
                <i class="fa fa-check-square"></i> Đồng ý
            </button>
            <button type="button" class="btn btn-cancel" data-dismiss="modal"><i class="fa fa-ban"></i> Bỏ qua</button>
       
    </div>
</div>




