            }
        }
        else if (index === 6) {
            $('._span-dich-vu-yc').find('.note-title').html(text);
            if (text === null
                || text === ''
                || text === undefined) {
                $('.__span-dich-vu-yc').hide();
            }
            else {
                $('._span-dich-vu-yc').show();
            }
        }
        else if (index === 7) {
            $('._span-thoi-gian').find('.note-title').html(text);
            if (text === null
                || text === ''
                || text === undefined) {
                $('._span-thoi-gian').hide();
            }
            else {
                $('._span-thoi-gian').show();
            }
        }
        else if (index === 9) {
            $('._span-nhan-vien').find('.note-title').html(text);
            if (text === null
                || text === ''
                || text === undefined) {
                $('._span-nhan-vien').hide();
            }
            else {
                $('._span-nhan-vien').show();
            }
        }
        else if (index === 10) {
