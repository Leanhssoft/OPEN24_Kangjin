                || text === ''
                || text === undefined) {
                $('._span-ghi-chu').hide();
            }
            else {
                $('._span-ghi-chu').show();
            }
        }
    });
    $('#_table-b1 tbody tr td .check-bat-buoc').on('change', function () {
        var index = $(this).closest('td').closest('tr').index();
        if (index === 0) {
            $('._chinhanhlabel').find('span').toggle();
        }
        else if (index === 1) {
        }
        else if (index === 2) {
            $('._khachhang').find('label').find('span').toggle();
        }
        else if (index === 3) {
            if ($(this).is(':checked')) {
                $('._theonhom').find('.check-theo-nhom').prop('checked', true);
                $('._theonhom').find('.form-ip-so-luong').show();
            }
            else {
                $('._theonhom').find('.check-theo-nhom').prop('checked', false);
                $('._theonhom').find('.form-ip-so-luong').hide();
            }
        }
        else if (index === 4) {
            $('._dienthoai').find('label').find('span').toggle();
        }
        else if (index === 5) {
            $('._email').find('label').find('span').toggle();
        }
        else if (index === 6) {
            $('._dichvuyc').find('label').find('span').toggle();
        }
        else if (index === 7) {
            $('._thoigianlabel').find('span').toggle();
        }
        else if (index === 9) {
            $('._nhanvienlabel').find('span').toggle();
        }
        else if (index === 10) {
            $('._ghichu').find('label').find('span').toggle();
        }
    });
    $('#_table-b1 tbody tr td ._tex-goi-y').on('change', function () {
        var index = $(this).closest('td').closest('tr').index();
        var text = $(this).val();
        $('.b1-view ._table-paramaster-view .mr-optin-table ._list-paramester .form-group').each(function (i) {
            if (i === index) {
                $(this).find('.note-optin').find('.note-title').html(text);
                if (text === null
                    || text === ''
                    || text === undefined) {
                    $(this).find('.note-optin').hide();
                }
                else if ($(this).css('display') !== 'none') {
                    $(this).find('.note-optin').show();
                }
            }
        })
    });
    $('.check-hieu-luc').on('change', function () {
        if (!$(this).is(':checked')) {
            $('#datetime_TuNgay').prop("readonly", true);
            $('#datetime_DenNgay').prop("readonly", true);
            $('._thong-bao-hieu-luc').prop("readonly", true);
        }
        else {
            $('#datetime_TuNgay').prop("readonly", false);
            $('#datetime_DenNgay').prop("readonly", false); $('._thong-bao-hieu-luc').prop("readonly", false);
        }
    })
    var elem = document.getElementById("view-optin");
    var elem1 = document.getElementById("view-optin1");
    $('#view-optin .view-full-scren').on('click', function () {
        $('.view-full-scren').hide();
        $('#view-optin').find('.vew-screen-full').addClass('height-100');
        $('#view-optin').find('.vew-screen-full').addClass('scroll-view');
        $('#view-optin').find('.vew-screen-full').find('.lich-hen-otinform').addClass('view-full-optin-lh');
        if (elem.requestFullscreen) {
            elem.requestFullscreen();
        } else if (elem.mozRequestFullScreen) { /* Firefox */
            elem.mozRequestFullScreen();
        } else if (elem.webkitRequestFullscreen) { /* Chrome, Safari & Opera */
            elem.webkitRequestFullscreen();
        } else if (elem.msRequestFullscreen) { /* IE/Edge */
            elem.msRequestFullscreen();
        }
    })
    $('#view-optin1 .view-full-scren').on('click', function () {
        $('.view-full-scren').hide();
        $('#view-optin1').find('.vew-screen-full').addClass('height-100');
        $('#view-optin1').find('.vew-screen-full').addClass('scroll-view');
        $('#view-optin1').find('.vew-screen-full').find('.lich-hen-otinform').addClass('view-full-optin-lh');
        if (elem1.requestFullscreen) {
            elem1.requestFullscreen();
        } else if (elem1.mozRequestFullScreen) { /* Firefox */
            elem1.mozRequestFullScreen();
        } else if (elem1.webkitRequestFullscreen) { /* Chrome, Safari & Opera */
            elem1.webkitRequestFullscreen();
        } else if (elem1.msRequestFullscreen) { /* IE/Edge */
            elem1.msRequestFullscreen();
        }
    })
    if (document.addEventListener) {
        document.addEventListener('webkitfullscreenchange', exitHandler, false);
        document.addEventListener('mozfullscreenchange', exitHandler, false);
        document.addEventListener('fullscreenchange', exitHandler, false);
        document.addEventListener('MSFullscreenChange', exitHandler, false);
    }
    function exitHandler() {
        if (!document.webkitIsFullScreen && !document.mozFullScreen && !document.msFullscreenElement) {
            $('.view-full-scren').show();
            $('#view-optin').find('.vew-screen-full').removeClass('height-100');
            $('#view-optin1').find('.vew-screen-full').removeClass('height-100');
            $('#view-optin').find('.vew-screen-full').removeClass('scroll-view');
            $('#view-optin1').find('.vew-screen-full').removeClass('scroll-view');
            $('#view-optin').find('.vew-screen-full').find('.lich-hen-otinform').removeClass('view-full-optin-lh');
            $('#view-optin1').find('.vew-screen-full').find('.lich-hen-otinform').removeClass('view-full-optin-lh');
        }
    }
    $('.selec-person ul').on('click', 'li', function () {
        var text = $(this).find('span').text();
        $(this).closest('ul').closest('.selec-person').prev('.choose-person').find('span').text(text);
    });
</script>
