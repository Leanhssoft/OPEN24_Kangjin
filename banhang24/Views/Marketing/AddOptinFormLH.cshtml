<script>
    function back() {
        location.href = '/#/optinForm'
    }
    $(function () {
        $('#select_thiet_lap_thoi_gian').on('change', function () {
            if ($(this).val() === '1' || $(this).val() === 1) {
                $('._tr-thiet-lap-thoi-gian').hide();
                $('._calender_time').hide();
            }
            else {
                $('._tr-thiet-lap-thoi-gian').show();
                $('._calender_time').show();
            }
        });
        $('.sl_nhanvien').on('click', function () {
            $('.tree-conpany').toggle();
            $('.tree-select-staff').toggle();
        });
        $('.td-time-hour').mask('99:99').on("change", function (e) {
        });
        $.datetimepicker.setLocale('vi');
        $('#datetime_TuNgay').datetimepicker({
            timepicker: false,
            mask: true, // '9999/19/39 29:59' - digit is the maximum possible for a cell
            format: 'd/m/Y',
            //value: moment(time).format('DD/MM/YYYY')
        });
        $('#datetime_DenNgay').datetimepicker({
            timepicker: false,
            mask: true, // '9999/19/39 29:59' - digit is the maximum possible for a cell
            format: 'd/m/Y',
            //value: moment(time).format('DD/MM/YYYY')
        });
    });
    var check = true;
    $('#_table-b1 tbody tr td .check-su-dung').on('change', function () {
        var index = $(this).closest('td').closest('tr').index();
        if (index === 0) {
            debugger
            $('.tree-conpany').toggle();
            if (check === true) {
                $('.tree-select-staff').toggle();
                $('.save-select-staff').toggle();
                $('.staff-perform').find('a').toggle();
            }
            if ($('.tree-conpany').css('display') === 'none' && $('.tree-select-staff').css('display') === 'none') {
                $('.op-lh-w70').addClass('_opwid100');
            }
            else {
                $('.op-lh-w70').removeClass('_opwid100');
            }
        }
        else if (index === 1) {
            if ($("._xungho").closest('.form-ip-2').hasClass('xungho-enable')) 
                {
                $("._xungho").closest('.form-ip-2').removeClass('xungho-enable');
                }
                else{
                $("._xungho").closest('.form-ip-2').addClass('xungho-enable');
                }
        }
        else if (index === 2) {
            $('._khachhang').toggle();
        }
        else if (index === 3) {
            $('._theonhom').toggle();
        }
        else if (index === 4) {
            $('._dienthoai').toggle();
        }
        else if (index === 5) {
            $('._email').toggle();
        }
        else if (index === 6) {
            $('._dichvuyc').toggle();
        }
        else if (index === 7) {
            $('._thoigian').toggle();
        }
        else if (index === 9) {
            $('._nhanvien').toggle();
            if ($($('._nhanvien')[0]).css('display') !== 'none')
            {
                check = true;
            }
            else {
                check = false;
            }
            if ($('.tree-conpany').css('display') === 'none' && $('.tree-select-staff').css('display') === 'none') {
                $('.op-lh-w70').addClass('_opwid100');
            }
            else {
                $('.op-lh-w70').removeClass('_opwid100');
            }
        }
        else if (index === 10) {
            $('._ghichu').toggle();
        }
    });
    $('#_table-b1 tbody ').on('change', 'tr td ._tex-goi-y', function () {
        var index = $(this).closest('td').closest('tr').index();
        var text = $(this).val();
        if (index === 0) {
            $('._span-chi-nhanh').find('.note-title').html(text);
            if (text === null
                || text === ''
                || text === undefined) {
                $('._span-chi-nhanh').hide();
            }
            else {
                $('._span-chi-nhanh').show();
            }
        }
        else if (index === 2) {
            $('._span-khachhang').find('.note-title').html(text);
            if (text === null
                || text === ''
                || text === undefined) {
                $('._span-khachhang').hide();
            }
            else {
                $('._span-khachhang').show();
            }
        }
        else if (index === 3) {
            $('._span-theo-nhom').find('.note-title').html(text);
            if (text === null
                || text === ''
                || text === undefined) {
                $('._span-theo-nhom').hide();
            }
            else {
                $('._span-theo-nhom').show();
            }
        }
        else if (index === 4) {
            $('._span-phone').find('.note-title').html(text);
            if (text === null
                || text === ''
                || text === undefined) {
                $('._span-phone').hide();
            }
            else {
                $('._span-phone').show();
            }
        }
        else if (index === 5) {
