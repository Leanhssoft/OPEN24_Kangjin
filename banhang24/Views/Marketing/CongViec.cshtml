<style>
    .btn-tab {
        height: 35px;
        width: 120px;
        border: none;
        outline: none;
        margin-left: 0;
        background: inherit;
    }
        .btn-tab:hover {
            background: #ebebeb;
        }
    .btn-tab {
        border-bottom: 4px solid buttonface;
    }
        .btn-tab.active {
            border-top-left-radius: 3px;
            border-top-right-radius: 3px;
            border-bottom: 4px solid #ee6002 !important;
        }
    .op-tr-show:hover .btn-hover-edit {
        display: inline-block;
    }
    .btn-hover-edit {
        right: 1px;
        display: none;
        background: white !important;
        height: 32px;
        padding: 0px 7px !important;
        z-index: 99;
    }
        .btn-hover-edit:hover {
            background: inherit !important
        }
        .btn-hover-edit a {
            color: var(--color-primary);
            vertical-align: central;
            width: 30px;
            text-align: center;
            padding: 5px !important;
        }
            .btn-hover-edit a .tooltiptext {
                visibility: hidden;
                width: 60px;
                background: #808080;
                color: #fff;
                text-align: center;
                border-radius: 6px;
                padding: 5px 0;
                /* Position the tooltip */
                z-index: 99;
                width: 90px;
                top: 60px;
                left: 0;
                margin-left: 0;
            }
            .btn-hover-edit a:hover .tooltiptext {
                visibility: visible;
            }
            .btn-hover-edit a:hover {
                color: var(--color-main);
                border-radius: 50%;
                background: #ebebeb
            }
    .btn.btn btn-main {
        height: 30px;
        padding-top: 0 !important;
    }
    .btn btn-main:focus, .btn btn-main:hover {
        color: white;
        background: var(--color-main)
    }
    /*.fc-axis{
        width:40px !important;
    }*/
    .fc-axis .fc-widget-header {
        width: 80px !important;
    }
    .fc-content-skeleton td {
        padding: 0 0 0 5px !important;
    }
    .event-hover {
        z-index: 500;
    }
    #modalNhacNho .modal-dialog draggable {
        width: 500px !important;
    }
    .fc-event {
        line-height: 1.5;
        font-size: 1em;
    }
    .event-hover {
        width: 200px;
        white-space: pre-wrap;
        background: red;
        background: #d5bdac;
        position: absolute;
        z-index: 999
    }
        .event-hover i {
            width: 30px;
            position: relative;
            padding-left: 5px;
        }
        .event-hover div {
            border-bottom: 1px solid white;
            height: auto;
            padding: 3px 0px;
        }
    .fc-time-grid-event .fc-title {
        white-space: pre-wrap;
    }
    .question-delete {
        width: 100px;
        background: blue;
        background: #d5bdac;
        position: absolute;
        z-index: 10001
    }
    .btnCalendar button {
        color: black !important;
    }
    .timeline {
        position: absolute;
        color: red;
        border-top: 2px solid red;
        width: 100%;
        margin: 0;
        padding: 0;
        z-index: 9999999;
    }
    .spantimeline {
        border-radius: 5px
    }
    .spandot {
        width: 6px;
        height: 6px;
        float: left;
        border-radius: 4px;
        margin-top: 3px;
        margin-right: 4px;
    }
    .monthToday {
        height: 18px;
        width: 18px;
        color: white;
        text-align: center;
        float: right;
        border-radius: 18px;
        background: blue;
    }
    .fc-event[href], .fc-event.fc-draggable {
        cursor: pointer;
        border: 2px solid white !important;
        border-radius: 5px !important;
        padding: 5px !important;
    }
</style>
