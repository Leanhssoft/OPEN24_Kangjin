<script>
    var funcName = '@RoleKey.CongViec';
    $('.filter-muc-do').on('click', 'li a', function () {
        $('.filter-muc-do li ').each(function () {
            $(this).find('a').removeClass('active');
        });
        $(this).addClass('active')
    });
    var setTop = 0;
    var heigth = 0;
    var heightold = 0;
    $(".fc-event-container").height(911)
    // check expand detail
    var isNew = false;
    $('#table-congviec').on('click', '.op-tr-show td', function () {
        if ($(this).hasClass('check-group') || $(this).hasClass('btn-hover-edit')) {
            isNew = false;
        }
        else {
            isNew = true;
            var tr = $(this).parent();
            var trnext = $(tr).next(".op-tr-hide");
            var css = trnext.css('display');
            $(".op-tr-hide").hide();
            $(".op-tr-show").removeClass('tr-active');
            setTop = 52 + parseInt($(tr).height() * ($(tr).index() / 2));
            if (css === 'none') {
                $(tr).addClass('tr-active');
                trnext.toggle();
                heightold = parseInt($(tr).height());
                heigth = $(tr).next().height() + heightold;
                $('.line-right').height(heigth).css("margin-top", setTop + "px");
                $(tr).closest('tbody').closest('table').closest('.table-reponsive').removeClass('tablescroll');
                // active tab first
                trnext.find('.tab-content .tab-pane').removeClass('active');
                trnext.find('.tab-content .tab-pane:eq(0)').addClass('active');
                trnext.find('ul.nav-tabs li').removeClass('active');
                trnext.find('ul.nav-tabs li:eq(0)').addClass('active');
            }
            else {
                $('.line-right').height(0).css("margin-top", "0px");
                $(tr).closest('tbody').closest('table').closest('.table-reponsive').addClass('tablescroll');
            }
        }
    });
</script>
