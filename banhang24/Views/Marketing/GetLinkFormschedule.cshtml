<script>
    $(document).ready(function () {
        //debugger
        //window.localStorage.setItem('height-open24', $('#iframeId').context.body.scrollHeight);
        //var height = $($('iframe').context).find('html').find('body').find('._op24-lich-hen-otinform').height();
        //$($('iframe').context).height(height);
        $.datetimepicker.setLocale('vi');
        $('.datetime-lich-hen').datetimepicker({
            timepicker: false,
            mask: true,
            format: 'd/m/Y',
        });
        $('.calendar-datein').dcalendar().on('dateselected', function (e) { console.log(e.date); });

        $('.time-hour').mask('99:99').on("change", function (e) {
        });
    });
    $('._theonhom label .check-theo-nhom').change(function () {
        if ($(this).is(':checked')) {
            $(this).closest('label').next('.form-ip-so-luong').show();
        }
        else {
            $(this).closest('label').next('.form-ip-so-luong').hide();
        }
    });

    $('#sl_save_select_staff').on('click', function () {
        $('.tree-conpany').show();
        $('.tree-select-staff').hide();
    });
    $('._nhanvien').on('click', 'a.sl_nhanvien', function () {
        $('.tree-conpany').toggle();
        $('.tree-select-staff').toggle();
    });
    $('.lh-nhom').on('click', function () {
        $('.number-group').toggle();
    })
    $('.op-time-lh-left').on('click', 'button', function () {
        $('.op-time-lh-left button').each(function () {
            $(this).removeClass('ac');
        });
        $(this).addClass('ac');
    })
    $('.op-time-lh-head').on('click', 'a', function () {
        $('.op-time-lh-head a').each(function () {
            $(this).removeClass('ac');
        });
        $(this).addClass('ac');
    })
    $('.op-time-lh-right').on('click', '.op-time-select', function () {
        var clas = $(this).hasClass('ac');
        $('.op-time-lh-right .op-time-select').each(function () {
            $(this).removeClass('ac');
        })
        if (!clas)
        { $(this).addClass('ac'); }

    })
    $('.form-calender').on('click', '.calender-hour ul li', function () {
        $('.calender-ul-li').removeClass('active')
        $(this).closest('li').addClass('active')
    })
    $(" .note-optin").on('mouseenter', ' i', function () {
        $('.note-title').hide();
        if ($(this).next('.note-title').text() !== '' && $(this).next('.note-title').text() !== undefined && $(this).next('.note-title').text() !== null) {
            $(this).next('.note-title').show();
        }
    });
    $(".note-optin").on('mouseleave', function () {
        $('.note-title').hide();
    });

</script>
