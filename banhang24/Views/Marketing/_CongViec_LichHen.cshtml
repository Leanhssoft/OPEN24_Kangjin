                    <div class="floatleft">
                        <div class="col-md-6 col-xs-12 nopadding">
                            <div class="form-group floatleft">
                                <label class='css-form-label'>Trạng thái</label>
                                <div class="css-form-detail">
                                    <div class="dropdown" >
                                        <div  data-toggle="dropdown" aria-expanded="false">
                                            <div class="form-control" id="lblStatusWork" style="cursor:pointer">Đang xử lý</div>
                                        </div>
                                        <ul class="dropdown-menu " id="lstStatusWork" data-bind="foreach :$root.ListStatusWork">
                                            <li>
                                                <a data-bind="click : $root.Change_StatusWork" style="display:flex; justify-content:space-between;align-items:center; width:100%">
                                                    <div data-bind="text: Text, attr:{id: ID}"></div>
                                                    <i data-bind="attr:{id: 'spanCheckStatusWork_'+ ID}"></i>
                                                </a>
                                            </li>
                                        </ul>
                                    </div>

                                </div>
                            </div>
                        </div>
                        <div class="col-md-6 col-xs-12 nopadding">
                            <div class="form-group floatleft" data-bind="visible: newCongViec().TrangThai()!==1 && PhanLoai() == 4">
                                <label class="css-form-label" data-bind="text: newCongViec().TrangThai() == 2?'Ngày hoàn thành':'Ngày hủy' " style="padding-left: 15px"></label>
                                <div class="css-form-detail ">
                                    <input class="form-control datetimepicker text-center" data-bind="value: DateFinish" />
                                </div>
                            </div>
                        </div>
                    </div>
                    
                   
                    <div class="form-group floatleft" data-bind="visible: newCongViec().TrangThai() != 1">
                        <label class='css-form-label'>Kết quả</label>
                        <div class="css-form-detail">
                            <textarea rows="2" data-bind="value: newCongViec().KetQua" />
                        </div>
                    </div>
                </div>
    </div>
    <div class="modal-footer">
        <div class="container-fluid">
            <button type="button" class="btn btn-delete" data-bind="click: DeleteWork_Modal, visible: booleanAddCV() == false "><i class="fa fa-trash"></i>Xóa</button>
            <button type="button" class="btn btn-cancel" data-dismiss="modal"><i class="fa fa-ban"></i>Bỏ qua</button>
            <button type="button" class="btn btn-save" id="btnLuuCongViec" data-bind="click:addCongViec "><i class="fa fa-save"></i>Lưu</button>
        </div>
    </div>
</div>
