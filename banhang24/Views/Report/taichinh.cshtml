@{
    Layout = null;
}
@using System.Web.Optimization;
@using banhang24.Hellper;
<style>
    .treename {
        line-height: 30px;
        width: 100%;
        float: left;
    }
    ._tr-bold {
        font-weight: bold;
    }
    .trbolds td{
        font-weight:bold !important;
        border-bottom:1px solid black;
    }
    .tdMargin {
        padding-left: 20px !important;
    }
</style>
@Html.Hidden("loaibaocao", (int)commonEnum.GroupReport.taichinh, new { @id = "ID_loaibaocao" })
@Html.Hidden("soquy", (int)commonEnum.TypeReport.soquy, new { @id = "ID_soquy" })
<div class="op-container ">
    <div class="container-fluid nopadding">
        <div class="col-left">
            <div class=" op-filter ">
                <span class="title-report"> </span>
                <section class="op-filter-body">
                    <article class="boxLeft">
                        <h3 class="op-filter-title ">
                            <img src="/Content/images/icon/report-loaibaocao.png"> Loại báo cáo
                        </h3>
                        <aside class="op-filter-container">
                            <ul class="chose_kieubang ul-radius">
                                <li value="1" class="active" data-id="@((int)commonEnum.TypeReport.tonghop)">
                                    <a data-target="home" class="box-tab">
                                        <input type="radio" name="kieubang" value="1" id="BCTongHop">
                                        <label for="BCTongHop">Tổng hợp công nợ</label>
                                    </a>
                                </li>
                                <li value="2" data-id="@((int)commonEnum.TypeReport.thutien)">
                                    <a data-target="thutien">
                                        <input type="radio" name="kieubang" value="2" id="BCThuTien">
                                        <label for="BCThuTien">Nhật ký thu tiền</label>
                                    </a>
                                </li>
                                <li value="3" data-id="@((int)commonEnum.TypeReport.chitien)">
                                    <a data-target="chiten">
                                        <input type="radio" name="kieubang" value="3" id="BCChiTien">
                                        <label for="BCChiTien">Nhật ký chi tiền</label>
                                    </a>
                                </li>
                                <li value="4" data-id="@((int)commonEnum.TypeReport.soquy)">
                                    <a data-target="soquy">
                                        <input type="radio" name="kieubang" value="4" id="BCSoQuy">
                                        <label for="BCSoQuy">Sổ quỹ</label>
                                    </a>
                                </li>
                                <li value="5" data-id="@((int)commonEnum.TypeReport.taichinh)">
                                    <a data-target="taichinh">
                                        <input type="radio" name="kieubang" value="5" id="BCTaiChinh">
                                        <label for="BCTaiChinh">Báo cáo tài chính</label>
                                    </a>
                                </li>
                                <li value="6" data-id="@((int)commonEnum.TypeReport.phantichthuchi)">
                                    <a data-target="phantichthuchi">
                                        <input type="radio" name="kieubang" value="6" id="BCPhanTichThuCHi">
                                        <label for="BCPhanTichThuCHi">Phân tích thu chi</label>
                                    </a>
                                </li>
                            </ul>
                            <script>
                                $('.chose_kieubang li').on('click', function () {
                                    var tar = $(this).children('a').attr('data-target');
                                    console.log(tar);
                                    $('#' + tar).siblings('.tab-pane').hide();
                                    $('#' + tar).show();
                                })
                            </script>
                        </aside>
                    </article>
                    <article class="boxLeft showLoaiDoiTac">
                        <h3 class="op-filter-title " onclick="toggleSubFilter(this)">
                            <img src="/Content/images/icon/loaihang.png">  Loại đối tác
                        </h3>
                        <aside class="op-filter-container">
                            <div class="menuCheckbox choose_LoaiHang">
                                <ul>
                                    <li>
                                        <label><input value="1" type="checkbox" data-bind="checked: LoaiSP_HH"> Khách hàng</label>
                                    </li>
                                    <li>
                                        <label> <input value="2" type="checkbox" data-bind="checked: LoaiSP_DV"> Nhà cung cấp</label>
                                    </li>
                                    <li>
                                        <label>  <input value="3" type="checkbox" data-bind=""> Bảo hiểm</label>
                                    </li>
                                </ul>
                            </div>
                        </aside>
                    </article>
                    <article class="boxLeft " style="display:none">
                        <h3 class="op-filter-title " onclick="toggleSubFilter(this)">
                            <img src="/Content/images/icon/loaihang.png">  Loại phiếu
                        </h3>
                        <aside class="op-filter-container">
                            <div class="menuCheckbox ">
                                <ul>
                                    <li>
                                        <label><input value="1" type="checkbox" data-bind=""> Tất cả</label>
                                    </li>
                                    <li>
                                        <label> <input value="2" type="checkbox" data-bind=""> Phiếu thu</label>
                                    </li>
                                    <li>
                                        <label>  <input value="3" type="checkbox" data-bind=""> Phiếu chi</label>
                                    </li>
                                </ul>
                            </div>
                        </aside>
                    </article>
                    <article class="boxLeft " style="display:none">
                        <h3 class="op-filter-title " onclick="toggleSubFilter(this)">
                            <img src="/Content/images/icon/loaihang.png">  Hình thức thanh toán
                        </h3>
                        <aside class="op-filter-container">
                            <div class="menuCheckbox ">
                                <ul>
                                    <li>
                                        <label><input value="1" type="checkbox" data-bind=""> Tất cả</label>
                                    </li>
                                    <li>
                                        <label> <input value="2" type="checkbox" data-bind=""> Tiền mặt</label>
                                    </li>
                                    <li>
                                        <label>  <input value="3" type="checkbox" data-bind=""> Chuyển khoản</label>
                                    </li>
                                    <li>
                                        <label>  <input value="3" type="checkbox" data-bind="">Thẻ POS</label>
                                    </li>
                                </ul>
                            </div>
                        </aside>
                    </article>
                    <article class="boxLeft showChiNhanh" hidden>
                        <h3 class="op-filter-title " onclick="toggleSubFilter(this)">
                            <img src="/Content/images/icon/chinhanh-black.png">    Chi nhánh
                        </h3>
                        <aside class="op-filter-container">
                            <div class="outselect">
                                <div class="nameDonVi choose-person floatleft " data-toggle="dropdown" id="choose_DonVi">
                                    <ul data-bind="foreach: MangChiNhanh">
                                        <li class="li-input-span">
                                            <span data-bind="text: TenDonVi">
                                            </span>
                                            <span data-bind="click: $parent.CloseDonVi">
                                                <i class="fa fa-times"></i>
                                            </span>
                                        </li>
                                    </ul>
                                    <input type="text" class="dropdown nameDonVi" id="NoteNameDonVi" data-bind="event:{keyup: NoteNameDonVi}" placeholder="Chọn chi nhánh..." />
                                </div>
                                <div class="selec-person dropdown-menu" id="selec-all-DonVi">
                                    <ul>
                                        <li data-bind="click: SelectedDonVi">
                                            <span>Tất cả chi nhánh</span>
                                        </li>
                                        <!--ko foreach: ArrDonVi-->
                                        <li data-bind="attr:{id: ID}, click: $parent.SelectedDonVi">
                                            <span data-bind="text: TenDonVi "></span>
                                        </li>
                                        <!--/ko-->
                                    </ul>
                                </div>
                            </div>
                        </aside>
                    </article>
                    <article class="boxLeft showTimeTC" hidden>
                        <h3 class="op-filter-title " onclick="toggleSubFilter(this)">
                            <img src="~/Content/images/icon/thoigian.png" />   Thời gian
                        </h3>
                        <span></span>
                        <aside class="op-filter-container pinbut0">
                            <div class="menuCheckbox">
                                <div class="outselect ">
                                    <div class="choose-person" id="choose-person" data-toggle="dropdown" aria-expanded="true">
                                        <input class="form-control" type="text" data-bind="value: newYear" />
                                    </div>
                                    <div class="selec-person dropdown-menu" id="selec-all-Year" data-bind="foreach: ArrayYear">
                                        <ul>
                                            <li data-bind="attr:{id: Year}, click: $parent.SelectYearReport"><span data-bind="text: Year"></span></li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </aside>
                        <aside class="op-filter-container">
                            <div class="menuRadio chose_Time">
                                <ul>
                                    <li>
                                        <label><input type="radio" name="rdTonKho" value="1" data-bind="checked: Loc_TonKho"> Theo Tháng</label>
                                    </li>
                                    <li>
                                        <label><input type="radio" name="rdTonKho" value="2" data-bind="checked: Loc_TonKho"> Theo Quý</label>
                                    </li>
                                    <li>
                                        <label><input type="radio" name="rdTonKho" value="3" data-bind="checked: Loc_TonKho"> Theo Năm</label>
                                    </li>
                                </ul>
                            </div>
                        </aside>
                    </article>
                    <article class="boxLeft showNhomKH">
                        <h3 class="op-filter-title " onclick="toggleSubFilter(this)">
                            <img src="~/Content/images/icon/nguoiban.png" />     Nhóm khách hàng
                        </h3>
                        <span></span>
                        <aside class="op-filter-container">
                            <div class="outselect">
                                <div class="choose-person floatleft " data-toggle="dropdown" id="choose_NhomDT">
                                    <ul data-bind="foreach: MangNhomKhachHang">
                                        <li class="li-input-span">
                                            <span data-bind="text: TenNhomDoiTuong">
                                            </span>
                                            <span data-bind="click: $parent.CloseNhomKhachHang">
                                                <i class="fa fa-times"></i>
                                            </span>
                                        </li>
                                    </ul>
                                    <input type="text" class="dropdown" id="NoteNameNhomKhachHang" data-bind="event:{keyup: NoteNameNhomKhachHang}" placeholder="Chọn nhóm khách hàng..." />
                                </div>
                                <div class="selec-person dropdown-menu" id="selec-all-NhomKhachHang">
                                    <ul data-bind="foreach: NhomKhachHangs">
                                        <li data-bind="attr:{id: ID}, click: $parent.SelectedNhomKhachHang">
                                            <span data-bind="text: TenNhomDoiTuong"></span>
                                    </ul>
                                </div>
                            </div>
                        </aside>
                    </article>
                    <article class="boxLeft showNhomNCC">
                        <h3 class="op-filter-title " onclick="toggleSubFilter(this)">
                            <img src="~/Content/images/icon/nguoiban.png" />     Nhóm nhà cung cấp
                        </h3>
                        <span></span>
                        <aside class="op-filter-container">
                            <div class="outselect">
                                <div class="choose-person floatleft " data-toggle="dropdown" id="choose_NhomDT">
                                    <ul data-bind="foreach: MangNhomNhaCungCap">
                                        <li class="li-input-span">
                                            <span data-bind="text: TenNhomDoiTuong">
                                            </span>
                                            <span data-bind="click: $parent.CloseNhomNhaCungCap">
                                                <i class="fa fa-times"></i>
                                            </span>
                                        </li>
                                    </ul>
                                    <input type="text" class="dropdown" id="NoteNameNhomNhaCungCap" data-bind="event:{keyup: NoteNameNhomNhaCungCap}" placeholder="Chọn nhóm nhà cung cấp..." />
                                </div>
                                <div class="selec-person dropdown-menu" id="selec-all-NhomNhaCungCap">
                                    <ul data-bind="foreach: NhomNhaCungCaps">
                                        <li data-bind="attr:{id: ID}, click: $parent.SelectedNhomNhaCungCap">
                                            <span data-bind="text: TenNhomDoiTuong"></span>
                                    </ul>
                                </div>
                            </div>
                        </aside>
                    </article>
                    <article class="boxLeft showLoaiThuChi" hidden>
                        <h3 class="op-filter-title " onclick="toggleSubFilter(this)">
                            <img src="/Content/images/icon/loaithuchi.png">   Loại phiếu
                        </h3>
                        <span></span>
                        <aside class="op-filter-container">
                            <div class="outselect">
                                <div class="choose-person floatleft " data-toggle="dropdown" id="choose_LoaiThuChi">
                                    <ul data-bind="foreach: MangLoaiThuChi">
                                        <li class="li-input-span">
                                            <span data-bind="text: TenLoaiThuChi">
                                            </span>
                                            <span data-bind="click: $parent.CloseLoaiThuChi">
                                                <i class="fa fa-times"></i>
                                            </span>
                                        </li>
                                    </ul>
                                    <input type="text" class="dropdown" id="NoteNameLoaiThuChi" data-bind="event:{keyup: NoteNameThuChi}" placeholder="Chọn loại phiếu..." />
                                </div>
                                <div class="selec-person dropdown-menu" id="selec-all-LoaiThuChi">
                                    <ul data-bind="foreach: LoaiThuChis">
                                        <li data-bind="attr:{id: ID}, click: $parent.SelectedLoaiThuChi">
                                            <span data-bind="text: TenLoaiThuChi "></span>
                                    </ul>
                                </div>
                            </div>
                        </aside>
                    </article>
                    <article class="boxLeft DatePK showDate" hidden>
                        <h3 class="op-filter-title " onclick="toggleSubFilter(this)">
                            <img src="~/Content/images/icon/thoigian.png" />      Đến ngày
                        </h3>
                        <aside class="op-filter-container DatePK">
                            <div class="menuCheckbox choose_TimeReport">
                                <div class="form-group floatleft">
                                    <div class="floatleft form-wrap ">
                                        <input class="form-control" id="datetimepicker_mask" type="text">
                                    </div>
                                </div>
                            </div>
                        </aside>
                    </article>
                    <article class="boxLeft showTimeCN">
                        <h3 class="op-filter-title " onclick="toggleSubFilter(this)">
                            <img src="/Content/images/icon/thoigian.png">      Thời gian
                        </h3>
                        <span></span>
                        <aside class="op-filter-container">
                            <div class="menuCheckbox choose_TimeReport">
                                <div class="form-group floatleft">
                                    <div class="radio-menu">
                                        <input type="radio" name="rd_TimeReport" value="1" data-bind="checked: check_Time">
                                    </div>
                                    <div class="conten-choose">
                                        <div class=" choose-date">
                                            <div class="dr_TimeReport" data-toggle="dropdown" aria-expanded="true">
                                                <input type="text" class="dropdown form-control choose-date-show ip_TimeReport" placeholder="Hôm nay">
                                            </div>
                                            <div class="dropdown-menu choose_txtTime">
                                                <div class="col-md-4 col-sm-4">
                                                    <h3>Theo ngày và tuần</h3>
                                                    <ul>
                                                        <li value="1" style="display: block;"><a href="javascript:void(0);">Hôm nay</a></li>
                                                        <li value="2" style="display: block;"><a href="javascript:void(0);">Hôm qua</a></li>
                                                        <li value="3" style="display: block;"><a href="javascript:void(0);">Tuần này</a></li>
                                                        <li value="4" style="display: block;"><a href="javascript:void(0);">Tuần trước</a></li>
                                                    </ul>
                                                </div>
                                                <div class="col-md-4 col-sm-4">
                                                    <h3>Theo tháng và quý</h3>
                                                    <ul>
                                                        <li value="6" style="display: block;"><a href="javascript:void(0);">Tháng này</a></li>
                                                        <li value="7" style="display: block;"><a href="javascript:void(0);">Tháng trước</a></li>
                                                        <li value="9" style="display: block;"><a href="javascript:void(0);">Quý này</a></li>
                                                        <li value="10" style="display: block;"><a href="javascript:void(0);">Quý trước</a></li>
                                                    </ul>
                                                </div><div class="col-md-4 col-sm-4">
                                                    <h3>Theo năm</h3>
                                                    <ul>
                                                        <li value="11" class="ng-scope" style="display: block;"><a href="javascript:void(0);">Năm này</a></li>
                                                        <li value="12" class="ng-scope" style="display: block;"><a href="javascript:void(0);">Năm trước</a></li>
                                                    </ul>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group floatleft">
                                    <div class="radio-menu">
                                        <input type="radio" name="rd_TimeReport" value="2" data-bind="checked: check_Time">
                                    </div>
                                    <div class="conten-choose">
                                        <div class="floatleft form-wrap">
                                            <input type="text" class="form-control ip_DateReport newDateTime" name="daterange" disabled="disabled">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </aside>
                    </article>
                    <article class="boxLeft showKQKD" hidden>
                        <h3 class="op-filter-title " onclick="toggleSubFilter(this)">
                            <img src="/Content/images/icon/ketquakinhdoanh.png">       Kết quả kinh doanh
                        </h3>
                        <aside class="op-filter-container">
                            <div class="menuRadio chose_LocKinhDoanh">
                                <ul>
                                    <li>
                                        <label><input type="radio" name="rdKinhDoanhTM" data-bind="checked: check_LocKinhDoanh" value="1"> Đưa vào hạch toán</label>
                                    </li>
                                    <li>
                                        <label><input type="radio" name="rdKinhDoanhTM" data-bind="checked: check_LocKinhDoanh" value="2"> Không hạch toán</label>
                                    </li>
                                    <li>
                                        <label><input type="radio" name="rdKinhDoanhTM" data-bind="checked: check_LocKinhDoanh" value="3">Tất cả</label>
                                    </li>
                                </ul>
                            </div>
                        </aside>
                    </article>
                    <article class="boxLeft showLoaiTien" style="display:none">
                        <h3 class="op-filter-title " onclick="toggleSubFilter(this)">
                            <img src="~/Content/images/icon/thoigian.png" />   Hình thức thanh toán
                        </h3>
                        <aside class="op-filter-container">
                            <div class="menuRadio chose_LoaiTien">
                                <ul>
                                    <li>
                                        <label><input type="radio" name="rdLoaiTien" value="12" data-bind="checked: Loc_LoaiTien"> Tất cả</label>
                                    </li>
                                    <li>
                                        <label><input type="radio" name="rdLoaiTien" value="1" data-bind="checked: Loc_LoaiTien"> Tiền mặt</label>
                                    </li>
                                    <li>
                                        <label><input type="radio" name="rdLoaiTien" value="2" data-bind="checked: Loc_LoaiTien"> Ngân hàng</label>
                                    </li>
                                </ul>
                            </div>
                        </aside>
                    </article>
                </section>
                <div class="op-filter-footer">
                    <button type="button" class="btn btn-link" style="color:red" onclick="closeFilter()">Hủy</button>
                    <button type="button" class="btn btn-link" onclick="collapseSubFilter(this)">
                        <span>Thu gọn</span>
                    </button>
                    <button type="button" class="btn btn-link" style="display:none" onclick="expandSubFilter(this)">
                        <span>Mở rộng</span>
                    </button>
                    <button type="button" class="btn btn-main kv2Btn" onclick="closeFilter()">Hoàn tất</button>
                </div>
            </div>
        </div>
        <div class="col-right">
            <div class="op-header">
                <div class="" style="height:unset; text-align:center">
                    <h4 class="floatleft flex  flex-center">
                        <label data-bind="text: MoiQuanTam"></label>
                    </h4>
                    <span class="floatleft" data-bind="text: TodayBC"></span>
                    <span class="floatleft">Chi nhánh: <span data-bind="text: TenChiNhanh"></span></span>
                </div>
                <div class="flex flex-between">
                    <div class=" header-button">
                        <div class=" header-button-left">
                            <button class="btn btn-main btn-group btnExport  btn-excel-report" id="btnExport" name="btnExport" data-bind="click: ExportExcel">
                                <i class="fa fa-file-excel-o"></i> Xuất Excel
                            </button>
                            <button class="btn btn-main btn-group btnImport btn-print-report" style="display: none" type="submit" data-toggle="modal" data-target="#myModalinport" onclick="printPage()">
                                <img src="/Content/images/icon/print-black.png"> In báo cáo
                            </button>
                            <div class="btn-dropdown" id="select-column">
                                <button type="button" class="btn btn-main  dropdown-toggle" onclick="showCollumnFilter(this)" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                     <i class="fa fa-angle-double-down"></i>
                                </button>
                                <div class="dropdown-list" style="display: none;">
                                    <ul data-bind="foreach: listCheckbox">
                                        <li>
                                            <div class="checkbox">
                                                <label><input type="checkbox" class="checkbox" checked="checked" data-bind="value:Key"><span></span><label class="nopadding" data-bind="text:Value" /></label>
                                            </div>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class=" input-search-filter">
                        <button type="button" class=" btn-search-icon search-report" data-bind="click: LoadReport">
                            <i class="fa fa-search"></i>
                        </button>
                        <input type="text" class="form-control " id="txt_search" data-bind="event: {keyup: Select_Text}" placeholder="Theo mã, tên, số điện thoại đối tác">
                    </div>
                </div>
            </div>
            <div class="content-table" id="myScrollspy" >
                <div class="tab-content tab-show nopadding">
                    <div class="tab-pane active" id="home" data-id="@((int)commonEnum.TypeReport.tonghop)">
                        <div class="col-md-12 content-report">
                            <div class="table-reponsive table-frame" id="table-reponsive">
                                <table class="table table-striped table-hover">
                                    <thead>
                                        <tr>
                                            <th class=" @commonEnum.TypeReportFinancial.Financial_nhomdoitac mahang xntma middle bg17 text-left" rowspan="2">Nhóm đối tác</th>
                                            <th class=" @commonEnum.TypeReportFinancial.Financial_madoitac mahang xntma middle bg17 text-left" rowspan="2">Mã đối tác</th>
                                            <th class="  @commonEnum.TypeReportFinancial.Financial_tendoitac mahang xntma middle bg17 text-left" rowspan="2">Tên đối tác</th>
                                            <th class="  @commonEnum.TypeReportFinancial.Financial_dauky xntslnhap text-center bg18 text-center" colspan="2">Đầu kỳ</th>
                                            <th class="  @commonEnum.TypeReportFinancial.Financial_trongky  xntxuat bg20 text-center" colspan="2">Trong kỳ</th>
                                            <th class="  @commonEnum.TypeReportFinancial.Financial_cuoiky  xntxuat bg21 text-center" colspan="2">Cuối kỳ</th>
                                        </tr>
                                        <tr>
                                            <th class=" @commonEnum.TypeReportFinancial.Financial_dauky  bg18 text-right">Phải thu</th>
                                            <th class=" @commonEnum.TypeReportFinancial.Financial_dauky  bg18 text-right">Phải trả</th>
                                            <th class=" @commonEnum.TypeReportFinancial.Financial_trongky bg20 text-right">Phải thu</th>
                                            <th class=" @commonEnum.TypeReportFinancial.Financial_trongky bg20 text-right">Phải trả</th>
                                            <th class=" @commonEnum.TypeReportFinancial.Financial_cuoiky bg21 text-right">Phải thu</th>
                                            <th class=" @commonEnum.TypeReportFinancial.Financial_cuoiky bg21 text-right">Phải trả</th>
                                        </tr>
                                    </thead>
                                    <tbody data-bind="foreach: BaoCaoTaiChinh_CongNo_Page">
                                        <tr>
                                            <td class=" @commonEnum.TypeReportFinancial.Financial_nhomdoitac text-left" data-bind="text: NhomDoiTac"></td>
                                            <td>
                                                <a style="cursor: pointer" class="@commonEnum.TypeReportFinancial.Financial_madoitac text-left Trinhpv-hovver" data-bind="text: MaDoiTac, click: $parent.LoadDoiTac_byMaDT">
                                                </a>
                                            </td>
