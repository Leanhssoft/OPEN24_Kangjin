                                    <li data-bind="attr:{id: ID}, click: $parent.SelectedNhomDoiTuong">
                                        <a data-bind="text: TenNhomDoiTuong"></a>
                                    </li>
                                </ul>
                            </div>
                        </aside>
                    </article>
                    <article class="boxLeft sortTime showNameKH" hidden>
