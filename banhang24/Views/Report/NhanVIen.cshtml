<style>
    .seleted-img {
        padding-left: 130px;
    }
        .seleted-img .img-user-contact img {
            border: 1px dotted #ccc;
            width: 190px;
        }
        .seleted-img .choose-img {
            width: 190px;
        }
    .modal-footer-user {
        padding-top: 20px;
        padding-right: 45px;
        text-align: right;
    }
    .li-text-show {
        float: left;
    }
    .treename {
        line-height: 30px;
        width: 100%;
        float: left;
    }
    .icon-tree-li {
        position: absolute;
        left: 4px;
        top: 4px;
        width: 10px;
        font-size: 14px;
        color: var(--color-main);
    }
    .btn-main {
        margin-left: 0px;
    }
    .tree-phong-ban ul li p {
        width: 100%;
        padding-left: 15px;
    }
        .tree-phong-ban ul li p a {
            position: absolute;
            top: 5px;
            right: 5px;
            display: none;
        }
            .tree-phong-ban ul li p a .fa-pencil-square-o {
                font-size: 16px;
            }
    .tree-phong-ban ul {
        margin-left: 5px;
    }
    table tbody tr td a .fa-times {
        color: red;
        font-size: 16px;
        margin-left: 10px;
    }
    #selectPhongBan li div {
        padding-left: 15px;
    }
        #selectPhongBan li div:hover {
            color: #0097c4 !important;
        }
    #selectPhongBan ul {
        margin-left: 15px;
    }
    #selectPhongBan li:hover {
        background: white !important;
    }
    .thead-boder tr th {
        border: 1px solid white !important;
    }
    /*.ss-li ul {
        display: inherit;
    }*/
    .table-frame > table > thead > tr {
        background: unset !important;
    }
</style>
