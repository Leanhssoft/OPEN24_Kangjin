@model Open24.Controllers.ShopInfo
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Đăng ký thành công phần mềm open24";
}
@section scripts{
    @Scripts.Render("~/bundles/Knockout")
}
@section FormDangky{
    @Html.Action("_cuahangdangky", new { area = "", controller = "home" })
}
<div class="success">
   <div class="container">
    
          <div class="title-register text-center">
              <h2>Chúc mừng bạn đã đăng ký thành công !</h2>
          </div>
          <div class=" successfully">
             
              <div class="form-group borderbottom">
                  <div class="fl">Tên cửa hàng</div>
                  <div class="fl bold"><font color="#32b7b3" id="succTenCuaHang" >@Model.TenCuaHang</font></div>
              </div>
              <div class="form-group borderbottom">
                  <div class="fl">Tên đăng nhập</div>
                  <div class="fl bold"><font color="#32b7b3" id="succTenDangNhap">@Model.TenDangNhap</font></div>
              </div>
              <div class="form-group borderbottom">
                  <div class="fl">Địa chỉ truy cập gian hàng của bạn</div>
                  <div class="fl bold"><font color="#32b7b3" id="succDiaChiGianHang">@Model.DiaChiQuanLy</font></div>
              </div> 
              <div class="form-group  martop20">
                  <a id="btnBatDauKinhDoanh" href="@Model.DiaChiQuanLy" target="_blank" class="btn-updata ">Bắt đầu kinh doanh</a>
              </div>

          </div>
          <div class="regis-left right-ssuesss">
              <div class="fl img-regis text-center">
                  <img src="~/Content/images/imgmob/dangkythanhcong-23.png" alt="Đăng ký" />
              </div>
          </div>
     
   </div>
</div>

<script>
    window.onloadstart = window.history.pushState('', 'Open24.vn - Đăng ký thành công', '/dang-ky-thanh-cong');
</script>