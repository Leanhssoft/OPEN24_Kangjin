<script>
    $(document).ready(function () {
        $(document).on('click', '.tr-show', function () {
            var t = $(this).next(".tr-hide").css("display");
            if (t == "none") {
                $(".tr-hide").removeClass("block");
                $(this).next(".tr-hide").addClass("block");
            }
            else if (t == "block") {
                $(this).next(".tr-hide").removeClass("block");
            }
            else {
                $(".tr-hide").removeClass("block");
            }
        });
        });
    $('#selected-Status').on('change', function () {

        if ($(this).val() !== '2' && $(this).val() !== 2) {
            $('.pricea-ctived').show();
        }
        else {

            $('.pricea-ctived').hide();
        }
    });
</script>
