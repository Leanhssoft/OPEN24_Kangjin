@model Model_banhang24vn.CustomView.RoleStore
@using Model_banhang24vn.Common;
@{
    ViewBag.Title = "Danh sách cửa hàng đăng ký";
    Layout = "~/Areas/AdminPage/Views/Shared/_Layout.cshtml";

}
@section scripts {
    @Scripts.Render("~/bundles/Knockout")
    <script src="~/Assets/js/Admin/StoreRegistration.Index.js"></script>
    <script src="~/Assets/js/Admin/jsDateCompare.js"></script>
    <script>
        $(function () {
            $("#datetimepickerUpdate").datetimepicker({
                format: 'DD/MM/YYYY'
            });
            $("#datetimepickerUpdateNews").datetimepicker({
                format: 'DD/MM/YYYY'
            });
            $('.start-date-filter').datetimepicker({
                format: 'DD/MM/YYYY'
            });
            $('.end-date-filter').datetimepicker({
                format: 'DD/MM/YYYY'
            });
        });
        function convertrangthai(config) {
            if (config === undefined
                || config === null) {
                return "";
            }
            else if (config === true) {

                return "Đã ký hợp đồng";
            }
            else {
                return "Dùng thử";
            }
        }
    </script>




@Html.Partial("/Areas/AdminPage/Views/Home/_NapTienDichVuPartial.cshtml")
